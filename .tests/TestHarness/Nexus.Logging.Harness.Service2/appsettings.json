{
  "ApplicationName": "Service 2",
  "AppSettings": {
    "ENV": "DEV"
  },
  "Logging": {
      "MinimumLevel": "Info",
      "Filters": {
        "Microsoft": "Error",
        "System": "Error"
      },
      "OutputTemplate": "$ProgJsonLogFormatter",
      "Targets": [
        {
          "Provider": "Serilog",
          "Type": "Console"
        },
        {
          "Provider": "Serilog",
          "Type": "RollingFile",
          "Args": {
            "PathFormat": "LogOutput\\{ApplicationName}-{Date}.log",
            "FileCount": "15",
            "Shared": "true"
          }
        }
      ]
    },
    "AllowedHosts": "*"
  }
