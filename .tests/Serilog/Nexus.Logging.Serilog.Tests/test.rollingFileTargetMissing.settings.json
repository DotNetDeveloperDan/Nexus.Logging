{
  "ApplicationName": "Nexus.Logging.Serilog.Tests",
  "AppSettings": {
    "Environment": "UnitTests"
  },
  "Logging": {
    "MinimumLevel": "Info",
    "Filters": {
      "Microsoft": "Error",
      "System": "Error"
    },
    "OutputTemplate": "$NexusJsonLogFormatter",
    "Targets": [
      {
        "Provider": "Serilog",
        "Type": "RollingFile",
        "Args": {
          "FileCount": "2"
        }
      }
    ]
  }
}